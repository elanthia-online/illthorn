{
  "name": "illthorn",
  "productName": "illthorn",
  "version": "1.0.0",
  "description": "modern front-end for Gemstone IV",
  "main": "./src/main.js",
  "scripts": {
    "dev": "sass --watch src/app/styles/:src/dist",
    "start": "ENV=dev electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "test": "echo \"No testing configured\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [],
  "author": "benjamin",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/app/img/app-icon.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "illthorn"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "icon": "src/app/img/app-icon.png"
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "icon": "src/app/img/app-icon.png"
          }
        }
      ]
    }
  },
  "dependencies": {
    "debug": "^4.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^4.0.0",
    "electron-window-state": "^5.0.3",
    "keyboardjs": "^2.5.1",
    "linkifyjs": "^2.1.9",
    "mark.js": "^8.11.1",
    "mithril": "^1.1.6",
    "ps-list": "^6.3.0",
    "sass": "^1.26.10"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.39",
    "@electron-forge/maker-deb": "6.0.0-beta.39",
    "@electron-forge/maker-rpm": "6.0.0-beta.39",
    "@electron-forge/maker-squirrel": "6.0.0-beta.39",
    "@electron-forge/maker-zip": "6.0.0-beta.39",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.39",
    "electron": "5.0.6",
    "husky": "^4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "typescript": "^3.5.3"
  }
}
